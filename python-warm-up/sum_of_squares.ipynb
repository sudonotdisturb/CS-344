   "source": [
    "# Finds the sum of squares of the numbers in the list.\n",
    "def sum_of_squares(numberList):\n",
    "    sum = 0\n",
    "    for number in numberList:\n",
    "        sum += number ** 2\n",
    "    return sum"
   ]
  },
